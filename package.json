{
  "name": "pathfinder-js",
  "packageManager": "yarn@4.5.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "emcc": "source ./vendors/emsdk/emsdk_env.sh",
    "cpp": "emcc ./src/lib/*.cpp -o ./src/components/lib/index.js -s MODULARIZE -s EXPORT_ES6 -s ENVIRONMENT=web -s ALLOW_MEMORY_GROWTH -s EXPORTED_FUNCTIONS=\"[\"_malloc\", \"_free\"]\" --bind"
  },
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "dependencies": {
    "@types/node": "^22.8.7",
    "solid-js": "^1.9.3"
  },
  "devDependencies": {
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vite-plugin-solid": "^2.10.2"
  }
}
